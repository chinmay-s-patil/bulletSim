FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     sonicFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         0.005;

deltaT          1e-09;

writeControl    adjustableRunTime;

writeInterval   1e-08;

purgeWrite      0;

writeFormat     ascii;

adjustTimeStep  yes;

maxCo           5.0;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   10;

writePrecision  15;

runTimeModifiable true;

