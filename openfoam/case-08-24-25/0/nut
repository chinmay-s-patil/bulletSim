FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.4695743;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }

    open
    {
        type            calculated;       // computed from k and epsilon
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           uniform 0;
    }

    bullet
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }

    defaultFaces
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }
}

// ************************************************************************* //
